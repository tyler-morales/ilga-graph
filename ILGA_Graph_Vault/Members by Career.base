filters:
  and:
    - file.inFolder("Members")
    - 'file.name != "ILGA_Member_Index"'
views:
  - type: table
    name: "Members by Career Start"
    groupBy:
      property: career_start_year
      direction: ASC
    order:
      - career_start_year
      - chamber
      - party
      - role
      - file.name
